class Solution {
public:
    int numTrees(int n) {
        // Start typing your C/C++ solution below
        // DO NOT write int main() function
        int *f=new int [n+1];
        f[0]=1;
        f[1]=1;
        for(int i=2;i<=n;i++){
            f[i]=0;
            for(int j=i-1;j>=0;j--)       
                f[i]+=f[j]*f[i-j-1];              
        }
        return f[n];
    }
};
